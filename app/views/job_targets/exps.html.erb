<% page_title = "目标公司 #{@company_name} 的面经" %>
<% community_page_title(page_title) %>


<div class="blog_container">
	<a href="/job_targets/list/<%= session[:account_id] %>" class="float_r">
		返回我的求职目标</a>
		
	<h2><%= page_title %></h2>
	
	
	
	<div class="community_block_narrow" style="color: #555555;">
		<img src="/images/job_targets/steward_small.png" border="0" alt="求职管家" />
		
		&nbsp;&nbsp;
		
		为您找到了
		<span style="font-size: 14px; color: #000000;"><b><%= @exps.total_entries %></b></span>
		条
		目标公司
		<span style="font-size: 14px; color: #000000;"><strong><%= @company_name %></strong></span>
		的面经
	</div>
	
	<br />
	
	
	<div class="community_block">
		<%= paging_buttons(@exps) %>
	</div>
	
	<div class="community_block">
		<table cellpadding="0" cellspacing="0" border="0" class="main_part_w">
			<tr>
				<td valign="top">
					<%= render(:partial => "/exps/exp", :collection => @exps) %>
				</td>
				
				<td style="width: 30px;"></td>
				
				<td valign="top">
					<%= render :partial => "/exps/talks_area" %>
				</td>
			</tr>
		</table>
	</div>

	<div class="community_block">
		<%= paging_buttons(@exps) %>
	</div>
</div>


