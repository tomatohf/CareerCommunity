<table cellpadding="0" cellspacing="5" class="main_part_w">
	<tr>
		<td style="width: 50px">
			<a href="/groups/<%= group_id %>">
				<img src="<%= face_img_src(group_image) %>" border="0" /></a>
		</td>
		
		<td>
			<a href="/spaces/show/<%= admin_id %>" class="account_nick_link">
				<%= h(admin_nick) %></a>
