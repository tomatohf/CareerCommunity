<table cellpadding="0" cellspacing="0" class="main_part_w">
	<tr>
		<td>
			<a href="/spaces/show/<%= operator[2] %>" class="account_nick_link">
				<%= h(operator[0]) %></a>
			参与了投票
			<% if vote_topic_deleted %>
				&nbsp;&nbsp;
				[已被删除]
			<% else %>
				<a href="/votes/<%= vote_topic_id %>"><%= h(vote_topic_title) %></a>
			<% end %>
		</td>
		
		<% unless save_space %>
			<td style="width: 50px">
				<a href="/votes/<%= vote_topic_id %>">
					<img src="<%= face_img_src(vote_image) %>" border="0" /></a>
			</td>
		<% end %>
	</tr>
</table>

