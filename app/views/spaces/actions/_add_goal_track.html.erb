<table cellpadding="0" cellspacing="0" class="main_part_w">
	<tr>
		<td>
			<div>
				<a href="/spaces/show/<%= operator[2] %>" class="account_nick_link">
					<%= h(operator[0]) %></a>
				添加
				<a href="/goals/<%= goal_id %>">
					目标</a>
				的
				<a href="/goals/track/<%= track_id %>">
					进度</a>
				说:
				&nbsp;
				<% h_track_desc = h(track_desc) %>
				<span title="<%= h_track_desc %>">
					<img src="/images/left_quote.gif" border="0" />
						<%= truncate(h_track_desc) %>
					<img src="/images/right_quote.gif" border="0" />
				</span>
			</div>
		</td>
		
		<% unless save_space %>
			<td style="width: 50px">
				<a href="/spaces/show/<%= operator[2] %>">
					<img src="<%= face_img_src(operator[1]) %>" border="0" /></a>
			</td>
		<% end %>
	</tr>
</table>

